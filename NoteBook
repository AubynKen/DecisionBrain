import pandas as pd

df_employees = pd.read_excel('InstanceBordeauxV1.xlsx', sheet_name = 0)

print(df_employees)

print(df_employees.columns)

print(df_employees.EmployeeName)

print(df_employees.EmployeeName[0])

print(df_employees.shape)

employees = dict()
cols = df_employees.columns

for i in range(df_employees.shape[0]):
    employees[df_employees.EmployeeName[i]] = {cols[1] : df_employees.Latitude[i]}
    
print(employees)
